<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="mainAnchorPane" prefHeight="700.0" prefWidth="900.0" style="-fx-background-color: #0F8A5F;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.EndGameController">
   <children>
      <ImageView fitHeight="730.0" fitWidth="920.0" layoutX="-5.0" layoutY="59.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/kieran-white-SBdmQcW8qag-unsplash.jpg" />
         </image>
      </ImageView>
      <Label layoutX="241.0" layoutY="86.0" text="Game Over" textFill="#f5624d">
         <font>
            <Font name="System Bold" size="80.0" />
         </font>
      </Label>
      <Text fx:id="scoreText" fill="#f5624d" layoutX="293.0" layoutY="267.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Your score:">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <Button fx:id="quitButton" layoutX="463.0" layoutY="523.0" mnemonicParsing="false" onAction="#quitButtonOnAction" prefHeight="46.0" prefWidth="134.0" style="-fx-background-color: #c6be4b; -fx-text-fill: #ffffff; -fx-background-radius: 50;" text="Quit">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="293.0" layoutY="523.0" mnemonicParsing="false" onAction="#backButtonOnAction" prefHeight="46.0" prefWidth="134.0" style="-fx-background-color: #c6be4b; -fx-text-fill: #ffffff; -fx-background-radius: 50;" text="Back">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
      <ImageView fx:id="blueJuminoIcon" fitHeight="80.0" fitWidth="80.0" layoutX="-10.0" layoutY="-15.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/591d04d20f4a73b5494ac1172a63f68a74c0eaf4r1-1000-1000_hq.gif" />
         </image>
      </ImageView>
      <ImageView fx:id="purpleJuminoIcon" fitHeight="80.0" fitWidth="80.0" layoutX="40.0" layoutY="-15.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/tumblr_o52aqu3BLG1ttph68o1_500.gif" />
         </image>
      </ImageView>
      <Button fx:id="scoreBoardButton" layoutX="371.0" layoutY="450.0" mnemonicParsing="false" onAction="#scoreBoardButtonOnAction" prefHeight="46.0" prefWidth="159.0" style="-fx-background-color: #c6be4b; -fx-text-fill: #ffffff; -fx-background-radius: 50;" text="Score Board">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
